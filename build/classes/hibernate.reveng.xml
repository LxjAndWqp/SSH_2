<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-name=".*"/>
  <table-filter match-catalog="user" match-name="t_menu"/>
  <table-filter match-catalog="user" match-name="t_notice"/>
  <table-filter match-catalog="user" match-name="t_role"/>
  <table-filter match-catalog="user" match-name="t_role_menu"/>
  <table-filter match-catalog="user" match-name="t_user_role"/>
  <table-filter match-catalog="user" match-name="t_userinfo"/>
</hibernate-reverse-engineering>